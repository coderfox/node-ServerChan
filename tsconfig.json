{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true
  },
  "exclude": [
    ".vscode",
    "logs",
    "node_modules"
  ]
}